{
    "version": 1,
    "supportedProjectTypes": [ ],
    "id": "D.ROS",
    "category": "A.ROS",
    "trDescription": "Creates a basic ROS node.",
    "trDisplayName": "Basic Node",
    "trDisplayCategory": "ROS",
    "icon": "../../global/genericfilewizard.png",
    "enabled": "%{JS: [ %{Plugins} ].indexOf('ROSProjectManager') >= 0}",

    "options":
    [
        { "key": "FileName", "value": "%{JS: Util.fileName('%{TargetPath}', '.cpp')}" },
        { "key": "BaseName", "value": "%{JS: Util.completeBaseName('%{FileName}')}" }
    ],

    "pages" :
    [
        {
            "trDisplayName": "Location",
            "trShortTitle": "Location",
            "typeId": "File"
        },
        {
            "trDisplayName": "Project Management",
            "trShortTitle": "Summary",
            "typeId": "Summary"
        }
    ],
    "generators" :
    [
        {
            "typeId": "File",
            "data":
            {
                "source": "file.cpp",
                "target": "%{FileName}",
                "openInEditor": true
            }
        }
    ]
}

  <style name="Parentheses" foreground="#000000" background="#a3b3be"/>
  <style name="CurrentLine" background="#383e42" bold="true"/>
  <style name="CurrentLineNumber" foreground="#dcdcdc"/>
  <style name="Occurrences" foreground="#000000" background="#b4b4b4"/>
  <style name="Occurrences.Unused" foreground="#b3b3b3"/>
